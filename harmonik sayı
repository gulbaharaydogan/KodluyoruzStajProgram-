
import java.util.*;

public class kod {
	public static void main(String[] args) {
		
		int number;
		double res = 0.0 ;
        Scanner scan = new Scanner(System.in);
        
        System.out.print("Sayıyı Giriniz: "); 
        number = scan.nextInt();
        
        for(double i = 1; i <= number; i++){
            res += (1/i);
            
        }
        
        System.out.print("Sonuç : " + res);
    }
}
