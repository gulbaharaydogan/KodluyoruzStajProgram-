
import java.util.*;

public class kod {
	public static void main(String[] args) {
		int sayı;
		
        Scanner scan = new Scanner(System.in);
        System.out.println("sayı giriniz");
        sayı = scan.nextInt();

        int fib=0,ön=1;
        System.out.println(sayı +" Sayılı Fibonacci Serisi : ");
        
        for(int i = 0; i < sayı; i++){
        	
            System.out.print( fib + " " );
            
            fib = fib + ön;
            ön = fib - ön;

        }
	}}
